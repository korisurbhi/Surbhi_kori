#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int num,i,upto;
    cout<<"Enter number you want table:";
    cin>>num;
    cout<<"Enter upto how many numbers you want:";
    cin>>upto;
    cout<<"Multiplication table of "<<num<<" is :"<<endl;
    for(i=1;i<=upto;i++)
    {
        cout<<num<<"*"<<i<<"="<<num*i;
        cout<<"\n";
    }
    return 0;
}
